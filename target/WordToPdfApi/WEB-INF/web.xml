<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>WordToPdfServletApi</servlet-name>
  	<display-name>WordToPdfServletApi</display-name>
  	<description></description>
  	<servlet-class>com.wordtopdf.WordToPdfServletApi</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WindowsServletWordToPdf</servlet-name>
  	<display-name>WindowsServletWordToPdf</display-name>
  	<description></description>
  	<servlet-class>com.windowsPdfConvert.WindowsServletWordToPdf</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>WordToPdfServletApi</servlet-name>
  	<url-pattern>/WordToPdfServletApi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WindowsServletWordToPdf</servlet-name>
  	<url-pattern>/WindowsServletWordToPdf</url-pattern>
  </servlet-mapping>
</web-app>
